{
  "database": {
    "Dialect": "mysql",
    "Database": "wechat",
    "User": "root",
    "Password": "root",
    "Charset": "utf8mb4",
    /*要支持emoji的话，填 utf8mb4 */
    "Host": "127.0.0.1",
    /*数据库ip*/
    "Port": 3306,
    /*数据库端口*/
    "MaxIdleConns": 5,
    /*空闲时最大的连接数*/
    "MaxOpenConns": 10
    /*最大的连接数*/
  },
  "redis": {
    "Host": "127.0.0.1",
    /*redis所在机器的ip*/
    "Port": 6379,
    /*redis的端口*/
    "Password": "",
    /*redis的密码，若无密码，可不填*/
    "MaxIdle": 3,
    /*最大的空闲连接数*/
    "MaxActive": 5,
    /*最大的激活连接数*/
    "Database": 0
    /*选择存放数据库*/
  },
  "go": {
    "APIPoweredBy": "WeChat Robot API",
    /*后台go加的X-Powered-By*/
    "SiteName": "WeChat Robot",
    /*网站名称*/
    "Env": "development",
    /*模式(开发，测试，产品)*/
    "LogDir": "",
    /*日志文件所在的目录，如果不设的话，默认在项目目录下*/
    "APIPrefix": "/api",
    /*api服务请求前缀*/
    "Port": 8023,
    /*go监听的端口*/
    "ApiKey": "",
    /*图灵的APIKey*/
    "ApiSecret":""
  }
}